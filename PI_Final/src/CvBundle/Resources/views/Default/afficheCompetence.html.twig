    <div id="page" class="page">


        <!-- Main Content -->
        <div id="main" class="site-main">
            <!-- Page changer wrapper -->
            <div class="pt-wrapper">


                <!-- Subpages -->
                <div class="subpages">


                    <!-- Resume Subpage -->
                    <section class="pt-page pt-page-2" data-id="resume">
                        <div class="section-title-block">
                            <h2 class="section-title">Education</h2>
                        </div>
                    </section>

                </div>


                {% for c in list %}
                    <form>


                    {% if (c.NomComp != null) and (c.NomCertif != null)  and (c.langues != null) %}

                        <br>

                        <br>
                        <br>
                        <div class="timeline">

                            <div class="timeline-event te-primary">

                                <h4 class="event-name">compétences :</h4>
                                <br>
                                {{ c.NomComp }}

                                <h4 class="event-name">certificats : </h4>
                                <br>
                                {{ c.NomCertif }}

                                <h4 class="event-name">langues : </h4>
                                <br>
                                {{ c.langues }}

                            </div>
                        </div>

                    {% endif %}
                {% else  %}
                    {#<h4>vous n'avez pas encore rédiger votre CV </h4>#}
                    <script> alert( "vous n'avez pas encore rédiger votre CV ");</script>



                    </form>
                {% endfor %}
